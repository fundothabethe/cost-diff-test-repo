{
  "format_version": "1.2",
  "terraform_version": "1.6.0",
  "resource_changes": [
    {
      "address": "aws_instance.expensive_server",
      "mode": "managed",
      "type": "aws_instance",
      "name": "expensive_server",
      "change": {
        "actions": ["create"],
        "before": null,
        "after": {
          "instance_type": "c5.xlarge",
          "ami": "ami-0abcdef1234567890"
        }
      }
    },
    {
      "address": "aws_ebs_volume.large_storage",
      "mode": "managed",
      "type": "aws_ebs_volume",
      "name": "large_storage",
      "change": {
        "actions": ["create"],
        "before": null,
        "after": {
          "size": 500,
          "type": "gp3",
          "iops": 4000,
          "throughput": 200
        }
      }
    },
    {
      "address": "aws_db_instance.production_db",
      "mode": "managed",
      "type": "aws_db_instance",
      "name": "production_db",
      "change": {
        "actions": ["create"],
        "before": null,
        "after": {
          "instance_class": "db.r5.large"
        }
      }
    }
  ]
}
